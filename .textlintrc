{
  "filters": {
    "allowlist": {
      "allow": ["%"]
    }
  },
  "rules": {
    "max-ten": true,
    "no-mix-dearu-desumasu": {
      "preferInHeader": "ですます",
      "preferInBody": "ですます",
      "preferInList": "ですます",
      "strict": true
    },
    "spellcheck-tech-word": true,
    "preset-ja-technical-writing": true,
    "prh": {
      "rulePaths": [
        "./prh.yml"
      ]
    }
  }
}